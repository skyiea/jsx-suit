{
  "name": "jsx-suit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "npm install -g webpack webpack-dev-server && npm install",
    "server": "nodemon --watch server.js --exec babel-node -- server.js",
    "build": "NODE_ENV=production webpack -p --bail",
    "watch": "webpack --progress --watch",
    "watchs": "SOURCE_MAPS=on webpack --progress --watch",
    "hot": "webpack-dev-server --inline --hot --progress",
    "hots": "SOURCE_MAPS=on webpack-dev-server --inline --hot --progress"
  },
  "dependencies": {
    "babel": "^6.3.13",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.0",
    "babel-plugin-syntax-class-properties": "^6.3.13",
    "babel-plugin-syntax-decorators": "^6.3.13",
    "babel-plugin-syntax-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.26.2",
    "eslint": "^3.17.0",
    "eslint-config-skye": "^1.0.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^6.10.0",
    "express": "^4.10.2",
    "file-loader": "^0.10.1",
    "history": "^4.5.1",
    "node-sass": "^4.5.0",
    "nodemon": "^1.8.1",
    "react": "^15.4.2",
    "react-core-decorators": "^1.0.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.0",
    "react-router": "^3.0.2",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
}
