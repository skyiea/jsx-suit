{
  "name": "jsx-suit",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=6.2.0"
  },
  "scripts": {
    "prepare": "npm install -g webpack webpack-dev-server && npm install",
    "server": "nodemon --watch static-server.js static-server.js",
    "build": "NODE_ENV=prod webpack --config webpack.prod.config.js --progress --bail",
    "prod": "npm run build",
    "watch": "webpack --config webpack.dev.config.js --progress --watch",
    "hot": "webpack-dev-server --config webpack.dev.config.js --inline --hot --progress",
    "watchs": "SOURCE_MAPS=on npm run watch",
    "hots": "SOURCE_MAPS=on npm run hot"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel": "^6.3.13",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-module-method": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-regenerator-runtime": "^6.5.0",
    "bluebird": "^3.5.0",
    "css-loader": "^0.26.2",
    "eslint": "^3.17.0",
    "eslint-config-skye": "^1.0.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^6.10.0",
    "express": "^4.10.2",
    "file-loader": "^0.10.1",
    "history": "^4.5.1",
    "html-webpack-plugin": "^2.28.0",
    "keymirror": "^0.1.1",
    "lodash.isplainobject": "^4.0.6",
    "node-sass": "^4.5.0",
    "nodemon": "^1.8.1",
    "react": "^15.4.2",
    "react-core-decorators": "^1.0.2",
    "react-css-modules": "^4.1.0",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.2",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.2",
